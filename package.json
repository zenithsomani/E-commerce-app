{
  "name": "proshop",
  "version": "1.0.0",
  "description": "ecommerce application built with MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenithsomani/E-commerce-app.git"
  },
  "author": "Zenith Somani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zenithsomani/E-commerce-app/issues"
  },
  "homepage": "https://github.com/zenithsomani/E-commerce-app#readme",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "express": "^5.1.0",
    "mongoose": "^8.18.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.2",
    "nodemon": "^3.1.10"
  }
}
